{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Kubra Market Admin",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "cross-env",
                "NODE_ENV=development",
                "tsx",
                "server/index.ts"
            ],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "integratedTerminal"
        }
    ]
}